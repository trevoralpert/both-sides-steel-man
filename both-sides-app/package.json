{
  "name": "both-sides-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "check-all": "yarn type-check && yarn lint && yarn format:check && yarn test:ci",
    "load-test": "node src/__tests__/load-testing/run-load-tests.js",
    "load-test:websocket": "k6 run src/__tests__/load-testing/scenarios/websocket-load-test.js",
    "load-test:api": "k6 run src/__tests__/load-testing/scenarios/api-throughput-test.js",
    "load-test:realworld": "k6 run src/__tests__/load-testing/scenarios/real-world-simulation.js",
    "load-test:stress": "k6 run src/__tests__/load-testing/scenarios/websocket-load-test.js --env CONFIG=stressTestWebSockets",
    "load-test:list": "node src/__tests__/load-testing/run-load-tests.js list",
    "workflows:validate": "node .github/scripts/validate-workflows.js",
    "ci:validate": "yarn type-check && yarn lint && yarn format:check && yarn workflows:validate",
    "security:setup": "node scripts/security-setup.js",
    "security:audit": "yarn npm audit --severity moderate",
    "security:infrastructure": "node scripts/infrastructure-security-audit.js",
    "security:check": "yarn security:audit && yarn workflows:validate",
    "security:full": "yarn security:audit && yarn security:infrastructure",
    "health:check": "node scripts/health-check.js",
    "health:validate": "node scripts/health-check.js && echo 'Health validation passed'",
    "demo:generate": "node scripts/generate-demo-data.js",
    "demo:clean": "rm -rf demo-data",
    "production:validate": "npm run health:check && npm run security:check",
    "production:ready": "npm run production:validate && npm run demo:generate",
    "feedback:test": "node -e \"const { feedbackSystem } = require('./src/lib/feedback/feedback-system.ts'); console.log('Feedback system loaded successfully')\"",
    "analytics:test": "node -e \"const { advancedAnalytics } = require('./src/lib/analytics/advanced-analytics.ts'); console.log('Analytics system loaded successfully')\"",
    "feedback:export": "node -e \"const { feedbackSystem } = require('./src/lib/feedback/feedback-system.ts'); console.log(feedbackSystem.exportFeedbackData())\"",
    "analytics:export": "node -e \"const { advancedAnalytics } = require('./src/lib/analytics/advanced-analytics.ts'); console.log(advancedAnalytics.exportAnalyticsData())\"",
    "feedback:summary": "node -e \"const { feedbackSystem } = require('./src/lib/feedback/feedback-system.ts'); console.log(JSON.stringify(feedbackSystem.getFeedbackSummary(), null, 2))\"",
    "analytics:dashboard": "node -e \"const { advancedAnalytics } = require('./src/lib/analytics/advanced-analytics.ts'); console.log(JSON.stringify(advancedAnalytics.getDashboardData({start: new Date(Date.now() - 30*24*60*60*1000), end: new Date()}), null, 2))\""
  },
  "dependencies": {
